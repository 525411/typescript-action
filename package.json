{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "yarn tsc",
    "format": "yarn prettier --write '**/*.ts'",
    "format-check": "yarn prettier --check '**/*.ts'",
    "lint": "yarn eslint src/**/*.ts",
    "dist": "yarn ncc build --source-map --license licenses.txt",
    "test": "yarn jest",
    "all": "yarn build && yarn format && yarn lint && yarn test && yarn dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getsentry/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Sentry",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^16.10.5",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-config-sentry": "^1.41.0",
    "eslint-plugin-jest": "^25.3.2",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
